{
  "name": "nuxt",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@10.22.0",
  "engines": {
    "node": ">=22",
    "pnpm": ">=10"
  },
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare && npx simple-git-hooks",
    "lint": "npx eslint .",
    "typecheck": "nuxt typecheck"
  },
  "dependencies": {
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.2.0",
    "@nuxt/eslint": "^1.10.0",
    "@nuxt/ui": "^4.1.0",
    "@vueuse/core": "^14.0.0",
    "@vueuse/nuxt": "^14.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-format": "^1.0.2",
    "nano-staged": "^0.8.0",
    "nuxt": "^4.2.1",
    "simple-git-hooks": "^2.13.1",
    "typescript": "^5.9.3",
    "vue": "^3.5.24",
    "vue-router": "^4.6.3",
    "vue-tsc": "^3.1.4"
  },
  "pnpm": {
    "overrides": {
      "typescript": "^5.9.3",
      "vite": "^7.2.2"
    }
  },
  "simple-git-hooks": {
    "pre-commit": "npx nano-staged"
  },
  "nano-staged": {
    "*": "npx eslint . --fix"
  }
}
